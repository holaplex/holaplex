{
  "name": "holaplex-builder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production next start",
    "consumers:sign-metadata": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\" -T ./tasks/consumers/sign-metadata.ts",
    "lint": "next lint",
    "ts:lint": "npx tsc --noEmit",
    "prettier:run": "yarn prettier --write src",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\" -r tsconfig-paths/register",
    "dev:theme": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\" -T ./hacks/demo-theme.ts",
    "prepare": "husky install",
    "generate:graphql-apollo": "graphql-codegen -r dotenv-flow/config --config codegen-apollo.yml",
    "generate:graphql-request": "graphql-codegen -r dotenv-flow/config --config codegen-graphql-request.yml",
    "generate:graphql": "yarn generate:graphql-apollo && yarn generate:graphql-request",
    "storefronts:update": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\" -T ./hacks/update-store-json.ts"
  },
